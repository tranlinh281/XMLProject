<xsd:schema version="1.0"
           xmlns:xsd="http://www.w3.org/2001/XMLSchema"
           xmlns="https://linhtt.org/schema/listShoes"
           xmlns:shoes="https://linhtt.org/schema/shoes"
           targetNamespace="https://linhtt.org/schema/listShoes"
           elementFormDefault="qualified">
    <xsd:import namespace="https://linhtt.org/schema/shoes" schemaLocation="shoes.xsd"/>
    <xsd:element name="ListShoes" xmlns="https://linhtt.org/schema/listShoes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="shoes:Shoes" minOccurs="0" maxOccurs="unbounded" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
