<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns="https://linhtt.org/schema/shoes"
            targetNamespace="https://linhtt.org/schema/shoes"
            elementFormDefault="qualified">
    <xsd:element name="Shoes" xmlns="https://linhtt.org/schema/shoes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="linkProduct" type="xsd:string" />
                <xsd:element name="gender" type="xsd:string"/>
                <xsd:element name="brand" type="xsd:string"/>
                <xsd:element name="name" type="xsd:string"/>
                <xsd:element name="description" type="xsd:string"/>
                <xsd:element name="price" type="xsd:positiveInteger"/>
                <xsd:element name="imgSrc" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
